{
    "name": "fredmansky/eventsky",
    "description": "Craft plugin for event management and attendee registration",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "eventsky"
    ],
    "support": {
        "docs": "https://github.com/fredmansky/eventsky/blob/master/README.md",
        "issues": "https://github.com/fredmansky/eventsky/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Fredmansky",
            "homepage": "https://fredmansky.at"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0",
        "nystudio107/craft-plugin-vite": "^1.0"
    },
    "require-dev": {
      "craftcms/cms": "^3.2.0",
      "codeception/codeception": "^3.0",
      "vlucas/phpdotenv": "^3.0"
    },
    "autoload": {
        "psr-4": {
          "fredmansky\\eventsky\\": "src/"
        }
    },
    "extra": {
        "name": "Eventsky",
        "handle": "eventsky",
        "developer": "Fredmansky",
        "developerUrl": "https://fredmansky.at",
        "documentationUrl": "https://github.com/fredmansky/eventsky/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/fredmansky/eventsky/master/CHANGELOG.md",
        "components": {
            "eventskyService": "fredmansky\\eventsky\\services\\EventskyService"
        },
        "class": "fredmansky\\eventsky\\Eventsky"
    }
}
