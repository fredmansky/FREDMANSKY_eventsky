{
    "name": "fredmansky/eventsky",
    "description": "Craft plugin for event management and attendee registration",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "events",
        "eventsky",
        "event management"
    ],
    "authors": [
        {
            "name": "Fredmansky",
            "homepage": "https://fredmansky.at/"
        }
    ],
    "support": {
        "issues": "https://github.com/fredmansky/FREDMANSKY_eventsky/issues"
    },
    "license": "proprietary",
    "require": {
        "craftcms/cms": "^4.4"
    },
    "autoload": {
        "psr-4": {
          "fredmansky\\eventsky\\": "src/"
        }
    },
    "extra": {
        "name": "Eventsky",
        "handle": "eventsky",
        "hasCpSettings": true,
        "hasCpSection": true,
        "class": "fredmansky\\eventsky\\Eventsky"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "craftcms/rector": "dev-main"
    }
}
